@model List<BooksReadedMVC.Models.Author>

@{
    ViewBag.Title = "Index";
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table>
        @foreach (var author in Model)
        {
        <tr>
            <td>@author.IdAuthor</td>
            <td>@author.Name</td>
        </tr>
        }
    </table>
</body>
</html>
